{
  "name": "Visual Studio Code",
  "version": "1.2.3",
  "slug": "vscode",
  "description": "Fully featured VSCode experience, to edit your HA config in the browser, including auto-completion!",
  "url": "https://github.com/elcajon/addon-vscode",
  "ingress": true,
  "ingress_port": 1337,
  "docker_api": true,
  "homeassistant_api": true,
  "ports": {
    "1337/tcp": null,
    "22/tcp": null
  },
  "ports_description": {
    "1337/tcp": "VS-Code Web Port",
    "22/tcp": "Assign Port to activate SSH Server"
  },
  "panel_icon": "mdi:microsoft-visual-studio-code",
  "arch": [
    "amd64",
    "aarch64"
  ],
  "hassio_api": true,
  "hassio_role": "manager",
  "full_access": true,
  "map": [
    "config:rw",
    "ssl:rw",
    "addons:rw",
    "share:rw",
    "backup:rw",
    "media:rw"
  ],
  "services": [
    "mysql:want",
    "mqtt:want"
  ],
  "options": {
    "vscode": {
      "packages": [],
      "init_commands": []
    },
    "ssh": {
      "authorized_keys": [],
      "compatibility_mode": false
    }
  },
  "schema": {
    "vscode": {
      "packages": [
        "str"
      ],
      "init_commands": [
        "str"
      ]
    },
    "ssh": {
      "authorized_keys": [
        "str"
      ],
      "compatibility_mode": "bool"
    }
  },
  "image": "ghcr.io/elcajon/vscode/{arch}"
}