{
  "name": "Factorio Server Manager",
  "version": "1.0.2",
  "slug": "fsm",
  "description": "Factorio Server Manager",
  "url": "https://github.com/elcajon/addon-fsm",
  "init": false,
  "arch": [
    "amd64"
